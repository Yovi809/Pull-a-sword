local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("Satria hub | pull a sword", "Default")

local tab = DrRayLibrary.newTab("Main Tab", "ImageIdHere")

tab.newButton("infinite yield", "Click me!", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
end)

tab.newToggle("auto farm", "Toggle! (prints the state)", true, function(toggleState)
    if toggleState then
        _G.auto = true
    
    
    while _G.auto do
local args = {
    [1] = "Click",
    [2] = false
}

game:GetService("ReplicatedStorage"):WaitForChild("ClickEvent"):FireServer(unpack(args))
wait(0.1)
end
    else
        _G.auto = false
    
    
    while _G.auto do
local args = {
    [1] = "Click",
    [2] = false
}

game:GetService("ReplicatedStorage"):WaitForChild("ClickEvent"):FireServer(unpack(args))
wait(0.1)
end
    end
end)

tab.newButton("Fly", "Click me!", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/YSL3xKYU'))()
end)

local tab = DrRayLibrary.newTab("Turkey", "ImageIdHere")

tab.newToggle("auto farm", "Turkey world", true, function(toggleState)
    if toggleState then
        _G.auto = true
    
    
    while _G.auto do
local args = {
    [1] = "Click",
    [2] = true
