end
    end
end)

local tab = DrRayLibrary.newTab("teleport", "ImageIdHere")

tab.newButton("teleport world 2", " Click me!", function()
    local args = {
    [1] = "1",
    [2] = workspace:WaitForChild("teman1287"),
    [3] = false
}

game:GetService("ReplicatedStorage"):WaitForChild("PortalC"):FireServer(unpack(args))
end)

